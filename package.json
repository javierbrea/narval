{
  "name": "narval",
  "version": "2.2.1",
  "description": "Multi test suites runner for Node.js packages. Docker based",
  "main": "index.js",
  "keywords": [
    "docker",
    "istanbul",
    "mocha",
    "sinon",
    "chai",
    "test",
    "testing",
    "test suites",
    "unit test",
    "acceptance test",
    "integration test",
    "end to end test",
    "standardjs",
    "coverage"
  ],
  "bin": {
    "narval": "./bin/narval",
    "narval-msc_mocha": "./bin/narval-msc_mocha",
    "narval-msc-istanbul": "./bin/narval-msc-istanbul"
  },
  "scripts": {
    "test": "./bin/narval",
    "coveralls": "cat ./.coverage/unit/lcov.info | coveralls"
  },
  "standard": {
    "ignore": [
      "test/integration/packages/**/*.js"
    ]
  },
  "dependencies": {
    "bluebird": "3.5.1",
    "boom": "7.2.0",
    "colors": "1.3.1",
    "commander": "2.17.1",
    "fs-extra": "7.0.0",
    "handlebars": "4.0.11",
    "js-yaml": "3.12.0",
    "lodash": "4.17.10",
    "mocha-silent-reporter": "1.0.0",
    "mocha-sinon-chai": "3.0.0",
    "standard": "11.0.1",
    "strip-ansi": "4.0.0",
    "tracer": "0.9.1",
    "tree-kill": "1.2.2",
    "wait-on": "2.1.0",
    "yaml-include": "1.2.0"
  },
  "devDependencies": {
    "coveralls": "3.0.2",
    "mockery": "2.1.0"
  },
  "author": {
    "name": "Javier Brea",
    "url": "https://javierbrea.com"
  },
  "license": "MIT",
  "repository": "https://github.com/javierbrea/narval"
}
